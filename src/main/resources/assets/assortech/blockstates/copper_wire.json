{
  "multipart": [
    {
      "apply": { "model": "assortech:block/copper_wire_center" }
    },
    {
      "when": { "down": true },
      "apply": { "model": "assortech:block/copper_wire_connection", "uvlock": true }
    },
    {
      "when": { "up": true },
      "apply": { "model": "assortech:block/copper_wire_connection", "uvlock": true, "x": 180 }
    },
    {
      "when": { "north": true },
      "apply": { "model": "assortech:block/copper_wire_connection", "uvlock": true, "x": 90, "y": 180 }
    },
    {
      "when": { "south": true },
      "apply": { "model": "assortech:block/copper_wire_connection", "uvlock": true, "x": 90 }
    },
    {
      "when": { "west": true },
      "apply": { "model": "assortech:block/copper_wire_connection", "uvlock": true, "x": 90, "y": 90 }
    },
    {
      "when": { "east": true },
      "apply": { "model": "assortech:block/copper_wire_connection", "uvlock": true, "x": 90, "y": 270 }
    }
  ]
}